.square{width:100%;height:100vh;overflow:hidden;position:relative;box-sizing:border-box;background:url(https://sbx-server.oss-cn-shenzhen.aliyuncs.com/img/cum/squarebg.png) no-repeat center center;background-size:cover}.square .fallback-background{position:absolute;top:0;left:0;width:100%;height:100%;background:linear-gradient(135deg,#667eea,#764ba2);z-index:-1}.square .square-recommend{margin:32rpx 24rpx;width:30%}.square .goTopBox{width:100rpx;height:100rpx;border-radius:50%;display:flex;justify-content:center;align-items:center;position:fixed;right:10rpx;bottom:350rpx}.example{z-index:99999;background:transparent;border-radius:50%;display:flex;justify-content:center;align-items:center;position:fixed;right:10rpx;bottom:200rpx}.transition-button{width:100%;flex:1;margin-bottom:10px}.example .transition{display:flex;justify-content:center;align-items:center;border-radius:50%;text-align:center}.go-top-icon{width:100rpx;height:100rpx}.show-item{height:calc(100% - 120rpx);overflow:hidden}.logged-in-container{height:100vh;display:flex;flex-direction:column}.scroll-Y{flex:1;height:0}.scroll-Y-item{padding-bottom:100rpx}.login-required{display:flex;flex-direction:column;align-items:center;justify-content:center;height:85vh;box-sizing:border-box}.empty-icon{width:200rpx;height:200rpx;margin-bottom:40rpx}.login-title{font-size:24rpx;font-weight:400;color:#1a1a1a}.login-btn{width:300rpx;height:80rpx;background:linear-gradient(135deg,#667eea,#764ba2);color:#fff;font-size:28rpx;font-weight:600;border:none;border-radius:40rpx;display:flex;align-items:center;justify-content:center;box-shadow:0 8rpx 20rpx rgba(102,126,234,.3);margin-top:80rpx}.login-btn:active{transform:scale(.98);box-shadow:0 4rpx 10rpx rgba(102,126,234,.3)}
