/* 底部TabBar样式 */
.tab-bar.data-v-16ee0c05 {
  background: #FFFFFF;
  border-top: 1rpx solid #f0f0f0;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  /* 安全区域适配 - 有安全区域时使用safe-area-inset-bottom，没有时使用40rpx */
  padding-bottom: max(40rpx, env(safe-area-inset-bottom));
  padding-top: 10rpx;
  box-sizing: border-box;
}
.tab-item.data-v-16ee0c05 {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  transition: all 0.3s;
  box-sizing: border-box;
}
.tab-item.data-v-16ee0c05:active {
  transform: scale(0.95);
}
.tab-icon.data-v-16ee0c05 {
  width: 50rpx;
  height: 50rpx;
  transition: all 0.3s;
}
.tab-text.data-v-16ee0c05 {
  font-size: 22rpx;
  margin-top: 6rpx;
  color: #666666;
  font-weight: 500;
  transition: all 0.3s;
  line-height: 1.2;
  /* 防止文字被截断 */
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 100%;
}
.tab-item.active .tab-text.data-v-16ee0c05 {
  color: #7363FF;
  font-weight: 600;
}